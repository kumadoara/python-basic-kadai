class Human:
    def __init__ (self,name,age):
        self.name = name
        self.age = age

#メソッド定義
    def set_name(self,name,age):
        self.name = name
        self.age = age

    def printinfo(self):
        print(self.name)
        print(self.age)

#インスタンス化
human = Human('name','age')

#メソッドにアクセスして実行
human.printinfo()
